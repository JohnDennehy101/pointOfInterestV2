<form id="filterTitleForm" action="/getMonumentByTitle" method="GET" style="display:inline">

    <div class="uk-margin">
        <select class="uk-select" id="titleSelectField">
            <option value="" disabled selected>Select Title</option>
            {{#each monuments}}
            <option>{{title}}</option>
            {{/each}}
        </select>
        <button class="uk-button uk-button-primary uk-margin-top" type="submit" formmethod="get">Filter by
            title</button>
</form>